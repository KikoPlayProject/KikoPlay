/* ------------------- QToolButton ------------------- */
QToolButton#PageButton{
       border-style:none;
       border-radius:0px;
       padding:5px;
       color:#989898;
       background-color: rgba(48,77,80,0);
}
QToolButton#PageButton:hover,
QToolButton#PageButton:pressed,
QToolButton#PageButton:checked{
       border-style:solid;
       border-width:0px 0px 0px 0px;
       border-color:#1CA0E4;
       color:#FFFFFF;
       background:#666a6e;
}
QToolButton#PageButton_O{
       border-style:none;
       border-radius:0px;
       padding:5px;
       color:#989898;
       background-color: rgba(48,77,80,0);
}
QToolButton#PageButton_O:hover,
QToolButton#PageButton_O:pressed,
QToolButton#PageButton_O:checked{
       border-style:none;
       color:#FFFFFF;
       background:#666a6e;
}
QToolButton#ListPageButton{
       border-style:none;
       border-radius:0px;
       padding:5px;
       color:#989898;
       background:#363636;
}
QToolButton#ListPageButton:hover,
QToolButton#ListPageButton:pressed,
QToolButton#ListPageButton:checked{
       border-style:solid;
       border-width:0px 0px 2px 0px;
       border-color:#32a7fe;
       color:#FFFFFF;
       /*background:#666a6e;*/
}
QToolButton#DialogPageButton{
       border-style:none;
       border-radius:0px;
       padding:5px;
       color:#989898;
       background:transparent;
}
QToolButton#DialogPageButton:hover,
QToolButton#DialogPageButton:pressed,
QToolButton#DialogPageButton:checked{
    border-style:solid;
    border-width:0px 0px 5px 0px;
    border-color:#1CA0E4;
    background:transparent;
@if{DarkMode}
    color:#ffffff;
@else
    color:#000000;
@endif
}
QToolButton#AnimeInfoPage{
    border-style:none;
    border-radius:0px;
    padding:5px;
    color:rgb(180,180,180);
    background:transparent;
}
QToolButton#AnimeInfoPage:hover,
QToolButton#AnimeInfoPage:pressed,
QToolButton#AnimeInfoPage:checked{
    border-style:solid;
    border-width:0px 0px 5px 0px;
    border-color:#1CA0E4;
    color:rgb(240,240,240);
    background:transparent;
}
QToolButton#ListEditButton{
       border-style:none;
       border-radius:0px;
       padding:5px;
       color:#989898;
       background:none;
}
QToolButton#ListEditButton:hover{
       color:#FFFFFF;
       background:#666a6e;
}
QToolButton#ListEditButton:pressed,
QToolButton#ListEditButton:checked{
       color:#FFFFFF;
       margin-top:2px;
}
QToolButton[hideMenuIndicator="true"]::menu-indicator {
    image: none;
}
QToolButton#ControlButton{
    border-style:none;
    border-radius:0px,0px,5px,5px;
    color:#c8c8c8;
    background:none;
}
QToolButton#ControlButton:hover,
QToolButton#ControlButton:pressed,
QToolButton#ControlButton:checked{
    border-color:#1CA0E4;
    color:#FFFFFF;
    background:#666a6e;
}
QToolButton#closelButton{
    border-style:none;
    border-radius:0px,0px,5px,5px;
    color:#c8c8c8;
    background:none;
}
QToolButton#closelButton:hover,
QToolButton#closelButton:pressed,
QToolButton#closelButton:checked{
    border-color:#1CA0E4;
    color:#fa3605;
    background:#666a6e;
}
QToolButton#LogoButton {
    background:transparent;
    border-style:none;
    color:rgb(200,200,200);
}
QToolButton#LogoButton:menu-indicator{
    subcontrol-position: right center;
    subcontrol-origin: padding;
}
QToolButton#LogoButton:hover {
    color:rgb(255,255,255);
}
QWidget#PopupPage QToolButton#DialogPageButton{
    border-style:none;
    border-radius:0px;
    padding:5px;
    color:#989898;
    background:transparent;
}
QWidget#PopupPage QToolButton#DialogPageButton:hover,
QWidget#PopupPage QToolButton#DialogPageButton:pressed,
QWidget#PopupPage QToolButton#DialogPageButton:checked{
    border-style:solid;
    border-width:0px 0px 5px 0px;
    border-color:#1CA0E4;
    color:#ffffff;
    background:transparent;
}
QDialog QToolButton#AnimeFilterOptionButton{
    border:none;
@if{DarkMode}
    color: rgb(214, 214, 214);
@else
    color: rgb(0, 0, 0);
@endif
}
QToolButton#FilterOptionButton, QToolButton#AnimeFilterOptionButton{
    border:none;
}
QToolButton#FilterOptionButton:hover, QToolButton#AnimeFilterOptionButton:hover{
    color:#0000ff;
}
QToolButton#FilterOptionButton::menu-indicator, 
QToolButton#AnimeFilterOptionButton::menu-indicator {
      width:0px;
}
QToolButton#DownloadInfoPage{
    border-style:none;
    border-radius:0px;
    padding:5px;
    color:rgb(180,180,180);
    background:transparent;
}
QToolButton#DownloadInfoPage:hover,
QToolButton#DownloadInfoPage:pressed,
QToolButton#DownloadInfoPage:checked{
    border-style:solid;
    border-width:0px 0px 5px 0px;
    border-color:#1CA0E4;
    color:rgb(240,240,240);
    background:transparent;
}


