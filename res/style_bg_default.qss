/* ------------------- QTreeView ------------------- */
QTreeView {
    border: 1px solid #dcdcdc;
@if{DarkMode}
    background-color:rgb(50, 50, 50);
    color:rgb(214, 214, 214);
    alternate-background-color: rgb(40, 40, 40);
@else
    background-color:rgb(255, 255, 255);
    color:rgb(0, 0, 0);
@endif
}
QTreeView#playlist {
    border: 0px solid rgb(50, 50, 50);
    background-color: rgb(57, 58, 60);
    selection-background-color: rgba(255, 255, 255,0);
    selection-color: rgb(0, 160, 230);
    show-decoration-selected: 1;
    outline:0px;
}
QTreeView#playlist::item {
    /*color: rgb(200,200,200);*/
    border: none;
    background-color: transparent;
}
QTreeView#playlist::item:hover, QTreeView#playlist::branch:hover {
    background-color: rgba(255, 255, 255, 40);
    /*color:rgb(255,255,255);*/
}
QTreeView#playlist::item:selected,QTreeView#playlist::branch:selected{
    background-color: rgba(255, 255, 255, 40);
    /*color: rgb(240, 240, 240);*/
}
QTreeView#playlist::branch {
    background-color: transparent;
}
QTreeView#playlist::branch:open:has-children {
    image: url(:/res/images/branch-open.svg);
}
QTreeView#playlist::branch:closed:has-children {
    image: url(:/res/images/branch-closed.svg);
}
QTreeView#danmulist {
    border: 0px solid rgb(50, 50, 50);
    background-color: rgb(57, 58, 60);
    selection-background-color: rgba(255, 255, 255,0);
    selection-color: rgb(0, 160, 230);
    outline:0px;
    show-decoration-selected: 1;
}
QTreeView#danmulist::item {
    border: none;
    background-color: transparent;
}
QTreeView#danmulist::branch {
    background-color: transparent;
}
QTreeView#danmulist::item:hover, QTreeView#danmulist::branch:hover {
    background-color: rgba(255, 255, 255, 40);
}
QTreeView#danmulist::item:selected, QTreeView#danmulist::branch:selected{
    background-color: rgba(255, 255, 255, 40);
}
QTreeView#playlist QScrollBar:vertical,QTreeView#danmulist QScrollBar:vertical {
    background: transparent;
    width: 10px;
}
QTreeView#playlist QScrollBar::handle:vertical,QTreeView#danmulist QScrollBar::handle:vertical {
    background: rgba(255,255,255,180);
    min-height: 70px;
    border-radius: 1px;
}
QTreeView#playlist QScrollBar::handle:vertical:hover,QTreeView#danmulist QScrollBar::handle:vertical:hover {
    background: rgba(255,255,255,255);
}
QTreeView#playlist QScrollBar::handle:vertical:pressed,QTreeView#danmulist QScrollBar::handle:vertical:pressed {
    background: rgba(255,255,255,200);
}
QTreeView#playlist QScrollBar::add-line:vertical,QTreeView#danmulist QScrollBar::add-line:vertical {
    background: none;
}
QTreeView#playlist QScrollBar::sub-line:vertical,QTreeView#danmulist QScrollBar::sub-line:vertical {
    background: none;
}
QTreeView#playlist QScrollBar::add-page:vertical,QTreeView#danmulist QScrollBar::add-page:vertical,
QTreeView#playlist QScrollBar::sub-page:vertical,QTreeView#danmulist QScrollBar::sub-page:vertical {
    background: none;
}
QTreeView#DownloadView{
    outline:0px;
    background-color: rgba(255,255,255,40);
    alternate-background-color: transparent;
    color:rgb(240,240,240);
}
QTreeView#LabelView {
    border: 0px solid rgb(50, 50, 50);
    background: rgba(57, 58, 60,120);
    selection-background-color: rgba(255, 255, 255,0);
    selection-color: rgb(0, 160, 230);
    show-decoration-selected: 1;
    outline:0px;
    qproperty-topLevelColor: rgb(64, 179, 253);
    qproperty-childLevelColor: rgb(240,240,240);
    qproperty-countFColor: rgb(240,240,240);
    qproperty-countBColor: rgb(113, 123, 136);
}
QTreeView#LabelView::item {
    border: none;
    background-color: transparent;
