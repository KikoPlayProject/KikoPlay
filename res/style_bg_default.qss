/* ------------------- QMenu ------------------- */
QMenu {
@if{DarkMode}
    background-color: rgb(80, 80, 80);
@else
    background-color: #f2f3f4;
@endif    
    border: 1px solid #adbac2;
}
QMenu::item {
    background-color: transparent;
    padding: 5px 35px 5px 25px;
@if{DarkMode}
    color: rgb(240,240,240);
@else
    color: rgb(0,0,0);
@endif
}
QMenu::item:disabled {
@if{DarkMode}
    color: rgb(150,150,150);
@else
    color: rgb(100,100,100);
@endif
}
QMenu::separator {
     height: 1px;
     background: #cecfd1;
     margin: 1px 0px 1px 0px;
}
QMenu::item:selected:enabled {
    background-color: #32a7fe;
    color:#ffffff;
}
QMenu::indicator {
    width:20px;
    height:20px;
    margin:1px;
    padding:2px 0px 2px 4px;
}
QMenu::indicator:checked {
@if{DarkMode}
    image: url(:/res/images/check-hover.svg);
@else
    image: url(:/res/images/check-normal.svg);
@endif
}
QMenu::indicator:checked:selected {
  image: url(:/res/images/check-hover.svg);
}


