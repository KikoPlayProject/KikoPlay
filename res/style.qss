/* ------------------- QMenu ------------------- */
QMenu {
@if{DarkMode}
    background-color: rgb(80, 80, 80);
@else
    background-color: #f2f3f4;
@endif
    border: 1px solid #adbac2;
}
QMenu::item {
    background-color: transparent;
    padding: 5px 35px 5px 25px;
@if{DarkMode}
    color: rgb(240,240,240);
@else
    color: rgb(0,0,0);
@endif
}
QMenu::item:disabled {
@if{DarkMode}
    color: rgb(150,150,150);
@else
    color: rgb(100,100,100);
@endif
}
QMenu::separator {
     height: 1px;
     background: #cecfd1;
