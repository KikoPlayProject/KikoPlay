/* ------------------- QSlider ------------------- */
QSlider#widgetProcessSlider{
    border-color: #bcbcbc;
    qproperty-cColor2: rgb(67,156,243);
}
QSlider#widgetProcessSlider::groove:horizontal {
     border: 1px solid #999999;
     height: 1px;
     margin: 0px 0;
     margin-left:8px;
     margin-right:8px;
     /*left: 8px; right: 8px;*/
 }
QSlider#widgetProcessSlider::groove:horizontal:hover {
    border: 1px solid #999999;
    height: 4px;
    margin: 0px 0;
    margin-left:8px;
    margin-right:8px;
    /*left: 8px; right: 8px;*/
}
QSlider#widgetProcessSlider::handle:horizontal {
    width: 10px;
    height:14px;
    background:rgb(230,230,230);
    border: 1px ;
    margin: -9px;
    border-radius:4px;
}
QSlider#widgetProcessSlider::add-page:horizontal{
    background: #bcbcbc;
}
QSlider#widgetProcessSlider::sub-page:horizontal{
    background: #439cf3;
}
QSlider#MiniProgressSlider{
    border-color: #bcbcbc;
    margin-left:8px;
    margin-right:8px;
}
QSlider#MiniProgressSlider::groove:horizontal {
     border: 1px solid #999999;
     height: 2px;
}
QSlider#MiniProgressSlider::add-page:horizontal{
    background-color: #bcbcbc;
}
QSlider#MiniProgressSlider::sub-page:horizontal{
    background-color: #439cf3;
}
QSlider#widgetVolumeSlider{
    border-color: #bcbcbc;
    margin-top: 4px;
    margin-bottom: 4px;
}
QSlider#widgetVolumeSlider::groove:horizontal {
    border: 1px solid #999999;
    height: 1px;
    margin: 0px 0;
    left: 8px; right: 8px;
}
QSlider#widgetVolumeSlider::handle:horizontal {
    height: 15px;
    width:15px;
    border-image: url(:/res/images/sliderThumb.png);
    margin: -8px -8px -8px -8px;
}
QSlider#widgetVolumeSlider::add-page:horizontal{
    background-color: rgb(126, 139, 150);
}
QSlider#widgetVolumeSlider::sub-page:horizontal{
    background-color: rgb(37, 168, 198);
}
QSlider#ColorSlider{
    border-color: #bcbcbc;
    margin-top: 4px;
    margin-bottom: 4px;
}
QSlider#ColorSlider::groove:horizontal {
    border: 1px solid #999999;
    height: 6px;
    margin: 0px 0;
}
QSlider#ColorSlider::handle:horizontal {
    width: 16px;
    margin-top: -8px;
    margin-bottom: -8px;
    border: 1px solid #aaaaaa;
    background: #fefefe;
}
QSlider#ColorSlider::handle:horizontal:hover {
   background: #eeeeee;
}

QSlider#PopupPageSlider{
    margin-top: 4px;
    margin-bottom: 4px;
}
QSlider#PopupPageSlider::groove:horizontal {
    height: 5px;
    background-color: rgba(255, 255, 255, 100);
    left: 8px; right: 8px;
}

QSlider#PopupPageSlider::handle:horizontal {
    height: 18px;
    width:18px;
    border-image: url(:/res/images/sliderThumb.png);
