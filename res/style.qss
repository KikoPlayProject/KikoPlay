/* ------------------- QSlider ------------------- */

QSlider#MiniProgressSlider{
    border-color: #bcbcbc;
    margin-left:8px;
    margin-right:8px;
}
QSlider#MiniProgressSlider::groove:horizontal {
     border: 1px solid #999999;
     height: 2px;
}
QSlider#MiniProgressSlider::add-page:horizontal{
    background-color: #bcbcbc;
}
QSlider#MiniProgressSlider::sub-page:horizontal{
    background-color: #439cf3;
}

QSlider#ColorSlider{
    border-color: #bcbcbc;
    margin-top: 4px;
    margin-bottom: 4px;
}
QSlider#ColorSlider::groove:horizontal {
    border-style: none;
    height: 12px;
    margin: 0px 0;
}
QSlider#ColorSlider::handle:horizontal {
    width: 10px;
    height: 10px;
    border-style: solid;
    border-width: 1px;
    border-color: rgb(255, 255, 255);
    background: rgba(255, 255, 255, 180);
    border-radius: 6px;
}
QSlider#ColorSlider::handle:horizontal:hover {
   background: rgba(255, 255, 255, 220);
}

QSlider#OptionSlider{
    border-color: #bcbcbc;
    padding-bottom: 25px;
}

QSlider#OptionSlider::groove:horizontal {
    background-color: rgba(255, 255, 255, 100);
    height: 5px;
}

QSlider#OptionSlider::handle:horizontal {
    height: 18px;
    width:18px;
    border-image: url(:/res/images/sliderThumb.png);
    margin-top:-7px;
    margin-bottom:-7px;
}
QSlider#OptionSlider::handle:hover {
    border-image: url(:/res/images/slider-hover.png);
}
QSlider#OptionSlider::handle:pressed {
    border-image: url(:/res/images/slider-pressed.png);
}
QSlider#OptionSlider::add-page:horizontal{
    background-color: rgba(255, 255, 255, 0);
}
QSlider#OptionSlider::sub-page:horizontal{
    background-color: rgba(255, 255, 255, 0);
}
ClickSliderColorStyle {
    @if{useThemeColor}
        qproperty-cColor2: rgb(@ThemeColorL1);
        qproperty-sliderColor: rgba(@ThemeColor, 100);
        qproperty-handleColor: rgba(@ThemeColor, 200);
    @else
        qproperty-sliderColor: rgba(0, 174, 236, 100);
        qproperty-handleColor: rgba(0, 162, 219, 200);
    @endif
}


/* ------------------- QTreeView ------------------- */
QTreeView {
    @if{hasBackground}
        background-color: rgba(255, 255, 255, 40);
    @else
        background-color: rgb(60, 60, 60);
    @endif
    color: rgb(240,240,240);
    alternate-background-color: transparent;
    outline:0px;
    show-decoration-selected: 1;
    border: 0px solid rgb(50, 50, 50);
    border-radius: 8px;
    selection-background-color: transparent;
}
QTreeView::item {
    border: none;
    background-color: transparent;
    padding-top: 6px;
    padding-bottom: 6px;
    color: rgb(240,240,240);
}
QTreeView::branch {
    background-color: transparent;
    margin: 4px;
}
QTreeView::branch:open:has-children {
    image: url(:/res/images/playlist-branch-open.svg);
}
QTreeView::branch:closed:has-children {
    image: url(:/res/images/playlist-branch-closed.svg);
}

QTreeView#playlist {
    @if{hasBackground}
        border-top: 1px solid rgb(150, 150, 150);
        border-bottom: 1px solid rgb(150, 150, 150);
    @else
        border-top: 1px solid rgb(90, 90, 90);
        border-bottom: 1px solid rgb(90, 90, 90);
    @endif

    border-left: none;
    border-right: none;
    border-radius: 0px;
    background-color: transparent;
    selection-color: rgb(0, 160, 230);
}
QTreeView#playlist::item {
    padding-top: 0px;
    padding-bottom: 0px;
}
QTreeView#playlist::branch {
    background-color: transparent;
    padding-left: 2px;
    margin: 0px;
}
QTreeView#playlist::branch:open:has-children {
    image: url(:/res/images/playlist-branch-open.svg);
}
QTreeView#playlist::branch:closed:has-children {
    image: url(:/res/images/playlist-branch-closed.svg);
}
QTreeView#danmulist {
    @if{hasBackground}
        border-top: 1px solid rgb(150, 150, 150);
        border-bottom: 1px solid rgb(150, 150, 150);
    @else
        border-top: 1px solid rgb(90, 90, 90);
        border-bottom: 1px solid rgb(90, 90, 90);
    @endif
    border-left: none;
    border-right: none;
    border-radius: 0px;
    background-color: transparent;
    selection-color: rgb(0, 160, 230);
}
QTreeView#danmulist::item {
    padding-top: 2px;
    padding-bottom: 2px;
    color: none;
}
QTreeView#danmulist::branch {
    background-color: transparent;
    margin: 0px;
}
QTreeView#LabelView {
    border: 0px solid rgb(50, 50, 50);
    background-color: rgba(57, 58, 60, 0);
    selection-color: rgb(0, 160, 230);
    qproperty-topLevelColor: rgb(64, 179, 253);
    qproperty-childLevelColor: rgb(220,220,220);
    qproperty-countFColor: rgb(240,240,240);
    @if{hasBackground}
        qproperty-countBColor: rgba(255, 255, 255, 40);
    @else
        qproperty-countBColor: rgb(74, 74, 74);
    @endif
    margin-right: 4px;
}
QTreeView#LabelView::item {
    padding-top:4px;
    padding-bottom:4px;
}
QTreeView#LabelView::branch {
    background-color: transparent;
    padding-left: 2px;
    margin: 0px;
}
QTreeView#LabelView::branch:open:has-children {
    image: url(:/res/images/playlist-branch-open.svg);
}
QTreeView#LabelView::branch:closed:has-children {
    image: url(:/res/images/playlist-branch-closed.svg);
}
QTreeView#LabelView::indicator{
    width: 16px;
    height: 16px;
}
QTreeView#LabelView::indicator:checked {
    image: url(:/res/images/label-check.svg);
}
QTreeView#LabelView::indicator:unchecked {
    image: url(:/res/images/label-uncheck.svg);
}
QTreeView#LabelView::indicator:indeterminate {
    image: url(:/res/images/label-partially.svg);
}
QTreeView#BgmListView {
    border: 1px solid #1abc9c;
    selection-color: rgb(0, 160, 230);
    border-radius: 8px;
    @if{hasBackground}
        background-color: rgba(255, 255, 255, 40);
        qproperty-normColor: rgba(255, 255, 255, 100);
        qproperty-hoverColor: rgba(255, 255, 255, 240);
    @else
        background-color: transparent;
        qproperty-normColor: rgb(180, 180, 180);
        qproperty-hoverColor: rgb(0, 155, 255);
    @endif
}
QTreeView#BgmListView::item {
    color: none;
    padding-top:8px;
    padding-bottom:8px;
}
QTreeView#BgmListView::item:hover, QTreeView#BgmListView::branch:hover,
QTreeView#BgmListView::item:selected, QTreeView#BgmListView::branch:selected {
    @if{hasBackground}
        background-color: rgba(255, 255, 255, 60);
    @else
        background-color: #466961;
    @endif
}
QTreeView#BgmListView::branch {
    background-color: #f2faf8;
    margin: 0px;
}
QTreeView#BgmListView::indicator:checked {
    image: url(:/res/images/focus-on.svg);
}
QTreeView#BgmListView::indicator{
    width: 32px;
    height: 32px;
}
QTreeView#BgmListView::indicator:unchecked {
    image: url(:/res/images/focus-off.svg);
}

QTreeView#DanmuView::item {
    color: none;
}
TorrentTreeView {
    qproperty-normColor: rgb(240,240,240);
    qproperty-ignoreColor: rgb(160,160,160);
}
TorrentTreeView::item {
    color: none;
}
PeerTreeView {
    qproperty-borderColor: rgb(200, 200, 200);
    qproperty-backgroundColor: rgba(255, 255, 255, 120);
    @if{useThemeColor}
        qproperty-barColor: rgba(@ThemeColor, 140);
    @else
        qproperty-barColor: rgba(51, 168, 255, 200);
    @endif
}



/* ------------------- QHeaderView ------------------- */
QHeaderView {
    @if{hasBackground}
        @if{useThemeColor}
            background-color: rgba(@ThemeColorL2, 40);
        @else
            background-color: rgb(100,100,100);
        @endif
    @else
        background-color: rgb(100,100,100);
    @endif
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
}
QHeaderView::section {
    @if{hasBackground}
        background-color: transparent;
        @if{useThemeColor}
            border: 1px solid rgba(@ThemeColorL2, 60);
            border-right: 0px solid rgba(@ThemeColorL2, 60);
        @else
            border: 1px solid #6c6c6c;
            border-right: 0px solid #6c6c6c;
        @endif
    @else
        background-color: rgb(100,100,100);
        border: 1px solid #6c6c6c;
        border-right: 0px solid #6c6c6c;
    @endif
    color: white;
    font-size: 15px;
    padding-left: 4px;
    padding-top: 2px;
    padding-bottom: 2px;
}
QHeaderView::section:vertical {
    @if{hasBackground}
        @if{useThemeColor}
            border-bottom: 0px solid rgba(@ThemeColorL2, 60);
        @else
            border-bottom: 0px solid #6c6c6c;
        @endif
    @else
        border-bottom: 0px solid #6c6c6c;
    @endif
}
QDialog QHeaderView {
    background-color: rgb(100,100,100);
}
QDialog QHeaderView::section {
    border: 1px solid #6c6c6c;
    border-right: 0px solid #6c6c6c;
}
QHeaderView::section:first {
    border-top-left-radius: 8px;
}
QHeaderView::section:last {
    border-top-right-radius: 8px;
    @if{hasBackground}
        @if{useThemeColor}
            border-right: 1px solid rgba(@ThemeColorL2, 60);
        @else
            border-right: 1px solid #6c6c6c;
        @endif
    @else
        border-right: 1px solid #6c6c6c;
    @endif
}
QHeaderView::section:vertical:first {
    border-top-left-radius: 0px;
}
QHeaderView::section:vertical:last {
    border-top-right-radius: 0px;
    @if{hasBackground}
        @if{useThemeColor}
            border-bottom: 1px solid rgba(@ThemeColorL2, 60);
        @else
            border-bottom: 1px solid #6c6c6c;
        @endif
    @else
        border-bottom: 1px solid #6c6c6c;
    @endif
}
QHeaderView#BgmListHeader {
    background-color: #1abc9c;
}
QHeaderView#BgmListHeader::section {
    background-color: #1abc9c;
    color: white;
    padding-left: 4px;
    border: none;
}


