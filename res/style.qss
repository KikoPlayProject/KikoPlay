/* ------------------- QToolButton ------------------- */
QToolButton#PageButton{
    border-style:none;
    border-radius:4px;
    padding:5px;
    color: rgb(160, 160, 160);
    background:transparent;
}
QToolButton#PageButton:hover,
QToolButton#PageButton:pressed,
QToolButton#PageButton:checked{
    color: rgb(220, 220, 220);
    background: rgba(255, 255, 255, 25);
}

QToolButton#ListEditButton{
    background:none;
    border-style:none;
    color:rgba(255,255,255,200);
}
QToolButton#ListEditButton::menu-indicator {
    image: none;
}
QToolButton#ListEditButton:hover,
QToolButton#ListEditButton:pressed,
QToolButton#ListEditButton:checked{
    @if{useThemeColor}
        color: rgb(@ThemeColorL2);
    @else
        color: #1CA0E4;
    @endif
}

QToolButton[hideMenuIndicator="true"]::menu-indicator {
    image: none;
}
QToolButton#FilterOptionButton{
    border:none;
    color: rgba(255, 255, 255, 160);
}
QToolButton#FilterOptionButton:hover {
    @if{useThemeColor}
        color: rgb(@ThemeColor);
    @else
        color:#1CA0E4;
    @endif
}
QToolButton#FilterOptionButton::menu-indicator {
    width:0px;
}

QToolButton#DownloadInfoPageButton{
    border-style:none;
    border-radius:4px;
    padding-top: 2px;
    padding-bottom: 2px;
    color:#989898;
    background:transparent;
}
QToolButton#DownloadInfoPageButton:hover,
QToolButton#DownloadInfoPageButton:pressed,
QToolButton#DownloadInfoPageButton:checked{
    color: rgb(250, 250, 250);
    background: rgba(255, 255, 255, 25);
}


