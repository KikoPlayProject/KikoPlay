 }


/* ------------------- QProgressBar ------------------- */
QProgressBar {
    border: 1px solid #5ca3b7;
    text-align: center;
    border-radius:2px;
}
QProgressBar::chunk {
    @if{useThemeColor}
        background-color: rgb(@ThemeColorL1);
    @else
        background-color: #25b3df;
    @endif
}
QProgressBar#DownloadToolProgress {
    border: none;
    text-align: center;
    background-color: transparent;
}
QProgressBar#DownloadItemProgress{
    background-color: rgba(255, 255, 255, 30);
    border: 0px;
    border-radius:2px;
}
QProgressBar#DownloadItemProgress::chunk {
    border-radius:2px;
}



/* --------------------QTab-------------------------------*/
QTabWidget::pane {
    border: 0px solid #2BA9EC;
    background: transparent;
    top: 0px;
}

QTabBar::tab {
    border-style: none;
    min-width:10ex;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding: 8px;
    margin-right: 1px;
    color: rgb(214, 214, 214);
}

QTabBar::tab:hover {
    background-color: rgba(255,255,255,20);
}

QTabBar::tab:selected {
    background-color: rgba(255,255,255,40);
    color: rgb(72,146,244);
}

QTabBar::tab:!selected {
    margin-top: 0px; /* make non-selected tabs look smaller */
}

QTabBar::close-button {
    image: url(:/res/images/tab-close.svg);
    margin: 4px;
}

QTabBar::close-button:hover {
    image: url(:/res/images/tab-close-hover.svg);
}


ElaTheme {
    @if{useThemeColor}
        qproperty-primaryNormalLight: rgb(@ThemeColorD1);
        qproperty-primaryNormalDark: rgb(@ThemeColor);
        qproperty-primaryHoverLight: rgb(@ThemeColorL1);
        qproperty-primaryHoverDark: rgb(@ThemeColorL2);
        qproperty-primaryPressLight: rgb(@ThemeColor);
        qproperty-primaryPressDark: rgb(@ThemeColorD1);
    @else
        qproperty-primaryNormalLight: rgb(0, 103, 192);
        qproperty-primaryNormalDark: rgb(76, 194, 255);
        qproperty-primaryHoverLight: rgb(25, 117, 197);
        qproperty-primaryHoverDark: rgb(71, 177, 232);
        qproperty-primaryPressLight: rgb(49, 131, 202);
        qproperty-primaryPressDark: rgb(66, 161, 210);
    @endif
}
